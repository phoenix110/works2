<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.cernol.works.web.decantingordersource.DecantingOrderSourceEdit"
        datasource="decantingOrderSourceDs"
        focusComponent="fieldGroup"
        messagesPack="com.cernol.works.web.decantingordersource">
    <dsContext>
        <datasource id="decantingOrderSourceDs"
                    class="com.cernol.works.entity.DecantingOrderSource"
                    view="decantingOrderSource-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="decantingOrderSourceDs">
            <column width="250px">
                <field property="container"/>
                <field property="quantity"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
